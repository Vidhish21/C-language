#include <stdio.h>
void main()
{
    int a[5],i,min=0,max=0;
    printf("enter values :");
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=0;i<4;i++)
    {
        if(a[i]<a[i+1])
        {
            min=min+a[i];
           
        }
    }
    for(i=1;i<5;i++)
    {
        if(a[i]<a[i+1])
        {
            max=max+a[i];
        }
    }
    printf("%d %d",min,max);
}

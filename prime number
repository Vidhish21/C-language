import java.util.*;
public class Main
{
	public static void main(String[] args) {
	int n,count=0,i;
	Scanner sc = new Scanner(System.in);
	n=sc.nextInt();
	for(i=1;i<=n;i++){
	    if(n%i==0){
	        count++;

	    }
	}
	if(count==2)
	System.out.println(n + " prime number");
	
	else
	    System.out.println(n + " not a prime number");
	
	}
}
